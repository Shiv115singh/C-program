#include<stdio.h>
int main(){
    int i,n,j,num,arr[10];
    printf("\nEnter the number of element in the array: ");
    scanf("%d",&n);
    printf("\nEnter the array element: ");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    printf("\n Enter the number to be inserted : ");
    scanf("%d",&num);
    for(i=0;i<n;i++){
        if(arr[i]>num){
            for(j=n-1;j>=i;j--){
                arr[j+1]=arr[j];
            }
            arr[i]=num;
            break;
        }
    }
    if(i==n){
        arr[i]=num;
    }
    n++;
    printf("\n The array after inserting of %d is: ",num);
    for(i=0;i<n;i++){
        printf("\t %d",arr[i]);
    }
    return 0;
}
