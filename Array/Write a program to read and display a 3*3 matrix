#include<stdio.h>
int main(){
    int i,j,mat[3][3];
    printf("Enter the elements of the matrix ");
    printf("\n************************************\n");
    for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            scanf("%d",&mat[i][j]);
        }
    }
    printf("The Element of the matrix are: ");
    printf("\n**************************************");
    for(i=0;i<3;i++){
        printf("\n");
        for(j=0;j<3;j++){
            printf("%d\t",mat[i][j]);
        }
    }
    return 0;
}
