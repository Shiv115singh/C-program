#include<stdio.h>
int main(){
    int n;
    printf("Enter the size of array: ");
    scanf("%d",&n);
    int arr[n];
    printf("\n Enter the array element: \n");
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n-1;i++){
        for(int j=1;j<=n-i-1;j++){
            if(arr[j-1]>arr[j]){
               int  temp=arr[j];
                arr[j]=arr[j-1];
                arr[j-1]=temp;
            }
        }
    }
    printf("\narray element are : \n");
    for(int i=0;i<n;i++){
        printf("%d\t",arr[i]);
    }
}
