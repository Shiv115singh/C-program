#include<stdio.h>
int main(){
    int n;
    printf("Enter the size of array: ");
    scanf("%d",&n);
    int arr[n];
    printf("\n Enter the array element: \n");
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(int i=1;i<n;i++){
      int temp = arr[i];
      int j;
      for(j=i-1; j>=0 && arr[j] > temp; j--){
        arr[j+1] = arr[j];
       }
      arr[j+1] = temp;
     }

    printf("\narray element are : \n");
    for(int i=0;i<n;i++){
        printf("%d\t",arr[i]);
    }
}
