#include<stdio.h>
int main(){
    int arr[10];
    int i=0,num,n,found=0;
    printf("\nEnter the number of element in array : ");
    scanf("%d",&n);
    printf("\nEnter the element of array : ");
    for(i=0;i<n;i++){
        printf("arr[%d]: ",i);
        scanf("%d",&arr[i]);
    }
    printf("\nEnter the searching number  : ");
    scanf("%d",&num);
   for(i=0;i<n;i++){
       if(arr[i]==num){
           found=1;
           printf("\n %d is found in the array at position = %d",num,i);
           break;
       }
   }
    if(found==0){
        printf("\n %d does not exist in the array",num);
    }
   
    return 0;
}
