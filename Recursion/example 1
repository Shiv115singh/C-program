#include<stdio.h>
int fun1(int);
int funt2(int);
int main(){
    int num;
    printf("Enter any Number: ");
    scanf("%d",&num);
    printf("output is: %d", fun1(num));
    return 0;
}
int fun1(int number){
    if(number==0){
        return number;
    }else{
        return funt2(number);
    }
}
int funt2(int x){
    return fun1(x-1);
}
