#include<stdio.h>
int main(){
    int num1 ,num2;
    printf("Enter the values  ");
    printf("\n num1 : ");
    scanf("%d",&num1);
    printf("\n num2 : ");
    scanf("%d",&num2);
    
    printf("\n num1 & num2 = %d",num1 & num2);
    printf("\n num1 | num2 = %d",num1 | num2);
    printf("\n ~num1 = %d",~num1);
    printf("\n  ~num2 = %d",~num2);
    printf("\n num1 ^ num2 = %d",num1 ^ num2);
    printf("\n num1 << num2 = %d",num1 << num2);
    printf("\n num1 >> num2 = %d",num1 >> num2);
    
    return 0;
}
