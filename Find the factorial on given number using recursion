#include<stdio.h>
int fact(int);
int main(){
    int num;
    printf("Enter any Number: ");
    scanf("%d",&num);
    printf("Factorial of given number is: %d", fact(num));
    return 0;
}
int fact(int number){
    if(number==1){
        return 1;
    }else{
        return(number * fact(number-1));
    }
}
