#include<stdio.h>
int main(){
    char ch;
    printf("Enter any character: ");
    scanf("%c",&ch);
    if(ch>='A'&&ch<='Z'){
        printf("Enter the character is upper case , the lower case it is : %c",(ch+32));
    }else{
        printf("Enter the character is lower case , the upper case it is : %c",(ch-32));
    }
    return 0;
}
